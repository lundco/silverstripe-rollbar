{
    "name": "lundco/rollbar",
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "description": "Rollbar integration for SilverStripe. Binds Rollbar to SilverStripe's error & exception handling subsystem.",
    "keywords": ["silverstripe", "rollbar", "logging", "logs", "log", "exception", "error"],
    "author": [
        {
            "name": "Carsten Meldgaard"
        }
    ],
    "require": {
        "rollbar/rollbar": "^1.6",
        "silverstripe/framework": ">=3.1.0,<4"
    },
    "config": {
        "process-timeout": 900
    },
    "extra": {
        "installer-name": "silverstripe-rollbar"
    },
    "minimum-stability": "dev"
}
